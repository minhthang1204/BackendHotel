{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "require-jsdoc": ["error", {
            "require": {
                "FunctionDeclaration": false,
                "MethodDefinition": false,
                "ClassDeclaration": false,
                "ArrowFunctionExpression": false,
                "FunctionExpression": false
            }
        }],
        "indent": ["error", 4],
        "new-cap" : "off",
        "key-spacing" : ["error", {
            "beforeColon" : true,
            "afterColon" : true
        }],
        "linebreak-style" : 0,
        "camelcase" : 0,
        "object-curly-spacing" : ["error", "always"]
        
    }
}
